class Solution {
    public double findMedianSortedArrays(int[] A, int[] B) {
        int m =A.length;
        int n =B.length;
        if(M>N){
            int[] temp =A;A=B;B=temp;
            int tmp = m; m=n;n=tmp;
        }
        int iMin=0,iMax = m,halfLen=(m+n+1)/2;
        while(iMin<=iMax){
            int i=(iMin+iMax)/2;
            int j =halfLen -i;
            if(i<iMax &&B[j-1]>A[i]){
                iMin=i+1;
            }
            else if ()
        }
    }
}
